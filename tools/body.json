{
    "apiVersion": "batch/v1",
    "kind": "Job",
    "metadata": {
        "name": "sor-api",
        "namespace": "default"
    },
    "spec": {
        "backoffLimit": 0,
        "template": {
            "spec": {
                "containers": [
                    {
                        "image": "sebastus/dotnetapisor:v0.1.0",
                        "name": "sor-api"
                    }
                ],
                "restartPolicy": "Never"
            }
        }
    }
}